          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The following example shows a two’s complement formation of a composite integer:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "var carry = true",
            "carry = low .formTwosComplementSubsequence(carry)",
            "carry = high.formTwosComplementSubsequence(carry)"
          ],
