          "text" : "Quotients of dividing by GCD",
          "type" : "heading"
        },
        {
          "code" : [
            "guard !result.isZero else { return }",
            "precondition(lhsQuotient == lhs \/ result)",
            "precondition(rhsQuotient == rhs \/ result)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
