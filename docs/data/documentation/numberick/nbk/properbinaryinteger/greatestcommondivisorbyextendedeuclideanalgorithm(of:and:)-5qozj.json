          "code" : [
            "if !iteration.isOdd {",
            "    precondition(result == (lhs &* lhsCoefficient &- rhs &* rhsCoefficient))",
            "}   else {",
            "    precondition(result == (rhs &* rhsCoefficient &- lhs &* lhsCoefficient))",
            "}"
          ],
