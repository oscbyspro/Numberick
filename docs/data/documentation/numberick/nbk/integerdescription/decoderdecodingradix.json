      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The description may contain a plus or minus sign (+ or -), followed by",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "an optional radix indicator (0b, 0o or 0x), then one or more numeric digits",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "(0-9) or letters (a-z or A-Z). If the description uses an invalid format,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "or its value cannot be represented, the result is nil.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "┌──────────── → ─────────────┐",
            "│ description │ integer      │",
            "├──────────── → ─────────────┤",
            "│    \"123\"    │ Int256( 123) │",
            "│ \"+0x123\"    │ Int256( 291) │",
            "│ \"-0x123\"    │ Int256(-291) │",
            "│ \"~OX123\"    │ nil          │",
            "└──────────── → ─────────────┘"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The decoding strategy is case insensitive.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        }
      ],
